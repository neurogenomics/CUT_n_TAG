<script type="text/javascript">
var handler=null;
// see https://github.com/mbostock/d3/wiki/Ordinal-Scales#category20c
var colors = d3.scale.category20c().domain(d3.range(0,20)).range()

function c0(index) { return "#9c9c9c"; }
function c1(index) { return "#bdbdbd"; }
function c2(index) { return colors[index % 16]; } // <-- note: uses only the first 16 colors

var elapsed="26m 13s"
var beginningMillis=1611685931616;
var endingMillis=1611687505112;
var data=[
{"label": "CHECK_DESIGN (design_assays.csv)", "times": [{"starting_time": 1611686032438, "ending_time": 1611686151830, "color":c1(0)}, {"starting_time": 1611686151830, "ending_time": 1611686152344, "color":c2(0), "label": "2m 4s \/ 6.6 MB"}, {"starting_time": 1611686152344, "ending_time": 1611686156836, "color":c1(0)}]},
{"label": "FASTQC (H3K27ac_R1_T1)", "times": [{"starting_time": 1611686165515, "ending_time": 1611686292071, "color":c1(1)}, {"starting_time": 1611686292071, "ending_time": 1611686444449, "color":c2(1), "label": "4m 39s \/ -"}]},
{"label": "FASTQC (H3K27me3_R1_T1)", "times": [{"starting_time": 1611686168514, "ending_time": 1611686292077, "color":c1(1)}, {"starting_time": 1611686292077, "ending_time": 1611686444498, "color":c2(1), "label": "4m 36s \/ -"}]},
{"label": "TRIMGALORE (H3K27me3_R1_T1)", "times": [{"starting_time": 1611686172814, "ending_time": 1611686292121, "color":c1(2)}, {"starting_time": 1611686292121, "ending_time": 1611686750515, "color":c2(2), "label": "9m 28s \/ 2.2 GB"}]},
{"label": "TRIMGALORE (H3K27ac_R1_T1)", "times": [{"starting_time": 1611686174614, "ending_time": 1611686415152, "color":c1(2)}, {"starting_time": 1611686415152, "ending_time": 1611686826808, "color":c2(2), "label": "10m 41s \/ 1.6 GB"}]},
{"label": "MAKE_TSS_BED (genes.bed)", "times": [{"starting_time": 1611686203213, "ending_time": 1611686272069, "color":c1(3)}, {"starting_time": 1611686272069, "ending_time": 1611686274169, "color":c2(3), "label": "1m 44s \/ 8.4 MB"}, {"starting_time": 1611686274169, "ending_time": 1611686307627, "color":c1(3)}]},
{"label": "MAKE_GENOME_FILTER (genome.fa)", "times": [{"starting_time": 1611686206013, "ending_time": 1611686272424, "color":c1(4)}, {"starting_time": 1611686272424, "ending_time": 1611686296046, "color":c2(4), "label": "1m 42s \/ 4.7 MB"}, {"starting_time": 1611686296046, "ending_time": 1611686307682, "color":c1(4)}]},
{"label": "get_software_versions", "times": [{"starting_time": 1611686208613, "ending_time": 1611686272810, "color":c1(5)}, {"starting_time": 1611686272810, "ending_time": 1611686280932, "color":c2(5), "label": "1m 40s \/ 193 MB"}, {"starting_time": 1611686280932, "ending_time": 1611686308115, "color":c1(5)}]},
{"label": "output_documentation", "times": [{"starting_time": 1611686209712, "ending_time": 1611686273065, "color":c1(6)}, {"starting_time": 1611686273065, "ending_time": 1611686275028, "color":c2(6), "label": "1m 40s \/ 25.3 MB"}, {"starting_time": 1611686275028, "ending_time": 1611686309263, "color":c1(6)}]},
{"label": "FASTQC (H3K27ac_R1_T1)", "times": []}
]


function getTickFormat() {
  var MIN = 1000 * 60
  var HOUR = MIN * 60
  var DAY = HOUR * 24
  var delta = (endingMillis - beginningMillis)

  if( delta < 2 * MIN ) {
    return {
      format: d3.time.format("%S"),
      tickTime: d3.time.seconds,
      tickInterval: 5,
      tickSize: 6
    }
  }

  if( delta < 2 * HOUR ) {
    return {
      format: d3.time.format("%M"),
      tickTime: d3.time.minutes,
      tickInterval: 5,
      tickSize: 6
    }
  }

  if( delta < 2 * DAY ) {
    return {
      format: d3.time.format("%H:%M"),
      tickTime: d3.time.hours,
      tickInterval: 1,
      tickSize: 6
    }
  }

  if( delta <= 7 * DAY ) {
    return {
      format: d3.time.format("%b %e %H:%M"),
      tickTime: d3.time.hours,
      tickInterval: 6,
      tickSize: 6
    }
  }

  return {
    format: d3.time.format("%b %e"),
    tickTime: d3.time.days,
    tickInterval: 1,
    tickSize: 6
  }
}

function getLabelMargin(scale) {
    $('<span class="labelSpan" style="display: none"></span>').appendTo('body');

    var labelMargin = 0
    $.each(data, function (key, value) {
      labelMargin = Math.max(labelMargin, $('.labelSpan').html(value.label).width());
    });

    $('.labelSpan').remove();

    return (labelMargin * scale);
}

function render() {
  handler=null;
  $("#timeline").empty()
  $('#label_elapsed').text(elapsed)
  $('#label_launch').text( d3.time.format('%d %b %Y %H:%M')(new Date(beginningMillis)) )

  var width = $(window).width();
  var chart = d3.timeline()
    .stack() // toggles graph stacking
    .margin({left:getLabelMargin(0.85), right:100, top:0, bottom:0})
    .tickFormat( getTickFormat() )
    .rowSeperators('#f5f5f5')
    .showTimeAxisTick()
    ;
  var svg = d3.select("#timeline").append("svg").attr("width", width).datum(data).call(chart);
}

function hrz() {
if( handler != null ) clearTimeout(handler)
  handler = setTimeout(render, 150);
}

$(document).ready(render)
$(window).resize(hrz); // resize the applet on window resize
</script>
